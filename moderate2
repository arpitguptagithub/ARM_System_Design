void fibonacci(int n) {
    IF (n <= 0) {
        print_String("Please enter a positive number.\n", 31);
        RETURN;
    }

    int a = 0;
    int b = 1;
    print_String("Fibonacci Series: ", 18);
    print_int(a);
    print_String(" ", 1);
    print_int(b);
    print_String(" ", 1);

    FOR (int i = 2; i < n; i++) {
        int next = a + b;
        print_int(next);
        print_String(" ", 1);
        a = b;
        b = next;
    }

    print_String("\n", 1);
    RETURN;
}

int main() {
    int terms = 10; // Number of terms in the Fibonacci series
    fibonacci(terms); // Call the fibonacci function
    RETURN 0;
}
